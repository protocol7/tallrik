{
	"BundleType": "Application",
	"BundleIdentifier": "wowlineupradio",
	"AppName": {
		"en": "WOW Line-up Radio"
	},
	"SupportedLanguages": [
		"en"
	],
	"Dependencies": {
		"api": "0.1.0"
	},
	"UserInstallable": true,
	"ApiPermissions": {
		"core" : ["private"],
		"social" : ["private"],
		"whatsnew" : ["private"]
	},
	"RequiredPermissions": [
		"https://graph.facebook.com",
		"http://localhost:9999",
		"https://fbcdn-profile-*.akamaihd.net",
		"http://*.fbcdn.net",
		"http://*.cloudfront.net",
		"http://*.spotify.com",
		"http://*.wayoutwest.se"
	],
	"AcceptedLinkTypes": [
		"playlist",
		"artist"
	],
	"AcceptedViewDropTypes": [
		"text"
	]
}
